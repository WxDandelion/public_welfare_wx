.donation .header{background-image:url('data:image/jpeg;base64,');background-repeat:no-repeat;background-size:100% 100%;width:100%;height:460rpx;font-size:72rpx;color:#fff;padding:31rpx 0 45rpx 0;box-sizing:border-box;text-align:center;font-family: 'Guildford Pro';}
.donation .header .currentScore{font-size:40rpx;color:rgba(255, 255, 255, 0.8);text-align:center;margin-bottom:11rpx;}
.donation .header .line{width:60rpx;height:3rpx;background-color:#fff;margin:20rpx auto 0 auto;}
.donation .header .nav{font-size:22rpx;color:rgba(255, 255, 255, 0.8);flex:1;margin-top:35rpx;}
.donation .header .nav .item{width:33.33%;text-align:center;}
.donation .header .nav .item .num{color:#fff;font-size:40rpx;margin-bottom:5rpx;font-family: 'Guildford Pro';}
.donation .wrapper .nav{flex:1;width:690rpx;border-radius:20rpx 20rpx 0 0;margin:-96rpx auto 0 auto;background-color:#f7f7f7;height:96rpx;font-size:30rpx;color:#bbb;}
.donation .wrapper .nav .item{text-align:center;width:50%;}
.donation .wrapper .nav .item.on{background-color:#fff;color:#e93323;font-weight:bold;border-radius:20rpx 0 0 0;}
.donation .wrapper .nav .item:nth-of-type(2).on{border-radius:0 20rpx 0 0;}
.donation .wrapper .nav .item .iconfont{font-size:38rpx;margin-right:10rpx;}
.donation .wrapper .list{background-color:#fff;padding:24rpx 30rpx;}
.donation .wrapper .list .tip{font-size:25rpx;width:690rpx;height:60rpx;border-radius:50rpx;background-color:#fff5e2;border:1rpx solid #ffeac1;color:#c8a86b;padding:0 20rpx;box-sizing:border-box;margin-bottom:24rpx;}
.donation .wrapper .list .tip .iconfont{font-size:35rpx;margin-right:15rpx;}
.donation .wrapper .list .item{height:124rpx;border-bottom:1rpx solid #eee;font-size:24rpx;color:#999;}
.donation .wrapper .list .item .state{font-size:28rpx;color:#282828;margin-bottom:8rpx;}
.donation .wrapper .list .item .num{font-size:36rpx;font-family: 'Guildford Pro';}
.donation .wrapper .list2{background-color:#fff;padding:24rpx 0;}
.donation .wrapper .list2 .item{background-image:linear-gradient(to right,#fff7e7 0%,#fffdf9 100%);width:690rpx;height:180rpx;position:relative;border-radius:10rpx;margin:0 auto 20rpx auto;padding:0 25rpx 0 180rpx;box-sizing:border-box;}
.donation .wrapper .list2 .item .pictrue{width:90rpx;height:150rpx;position:absolute;bottom:0;left:45rpx;}
.donation .wrapper .list2 .item .pictrue image{width:100%;height:100%;}
.donation .wrapper .list2 .item .name{width:285rpx;font-size:30rpx;font-weight:bold;color:#c8a86b;}
.donation .wrapper .list2 .item .earn{font-size:26rpx;color:#c8a86b;border:2rpx solid #c8a86b;text-align:center;line-height:52rpx;height:52rpx;width:160rpx;border-radius:50rpx;}
 .header .picTxt .pictrue{width:120rpx;height:120rpx;}
 .header .picTxt .pictrue image{width:100%;height:100%;border-radius:50%;border:2px solid #f5f5f5;margin-left:35rpx}
 .header .picTxt .text{width:434rpx;color:rgba(255,255,255,1);margin-left:35rpx;}
 .header .picTxt .text .name{font-size:32rpx;max-width:270rpx;margin-left:35rpx;margin-top:15rpx;height: 120rpx; line-height: 100rpx;}
 .header .picTxt .text .id{color:rgba(255,255,255,0.6);font-size:26rpx;margin-top:15rpx;margin-left:35rpx;}
 .header .picTxt .text .id .iconfont{font-size:30rpx;margin-left:12rpx;}
.header .icon-shezhi{font-size:36rpx;color:#fff;margin-top:-52rpx;}

.donation .wrapper .list .btn{font-size:30rpx;width:160rpx;height:60rpx;border-radius:50rpx;background-color:#e93323;border:1rpx solid #ffeac1;color:#c8a86b;padding:0 20rpx;box-sizing:border-box;margin-bottom:24rpx;margin-top:24rpx;margin-left:250rpx;}